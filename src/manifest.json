{
  "manifest_version": 3,
  "name": "YouTube Bookmarks",
  "description": "Better YouTube bookmarks",
  "version": "1.0.3",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_popup": "popup.html",
    "default_title": "YouTube Bookmarks"
  },
  "author": "Samo Å lag",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ]
}