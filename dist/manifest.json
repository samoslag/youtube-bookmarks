{
  "manifest_version": 2,
  "name": "YouTube Bookmarks",
  "description": "Visualize your YouTube bookmarks.",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_popup": "popup.html",
    "default_title": "YouTube Bookmarks"
  },
  "author": "Samo Å lag",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self' ; object-src 'self'"
}